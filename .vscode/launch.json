    "version": "0.2.0",
    "configurations": [


        {
            "name": "hw2- Build and Debug",
            "type": "cppdbg",
            "request": "launch",
            "program":"${workspaceRoot}/cs184/hw2-linuxosx/transforms",
            "args": ["hw1.txt"],
            "stopAtEntry": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "Build hw2",
            "cwd": "${workspaceRoot}/cs184/hw2-linuxosx/",
            "setupCommands": [{ "text": "-enable-pretty-printing", "description": "Enable GDB pretty printing", "ignoreFailures": true }]
        },
        
        {
            "name": "main project",
            "type": "cppdbg",
            "request": "launch",
            "program":"${workspaceRoot}/project/main",
            "args": ["hw1.txt"],
            "stopAtEntry": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "make",
            "cwd": "${workspaceRoot}/project",
            "setupCommands": [{ "text": "-enable-pretty-printing", "description": "Enable GDB pretty printing", "ignoreFailures": true }]
        }
    ]
}